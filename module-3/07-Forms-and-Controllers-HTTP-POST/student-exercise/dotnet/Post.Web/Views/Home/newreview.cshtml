@model Review
@{
    ViewData["Title"] = "newreview";
}

<h2>New Book Review</h2>

<form asp-controller="Home">
    <label asp-for="Username"></label>
    <input asp-for="Username" />

    <label asp-for="Rating"></label>
    <input asp-for="Rating" />

    <label asp-for="ReviewTitle"></label>
    <input asp-for="ReviewTitle" />

    <label asp-for="ReviewText"></label>
    <input asp-for="ReviewText" />

    <input type="submit" value="Submit!" />
</form>
